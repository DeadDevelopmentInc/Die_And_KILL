<Window x:Class="Die_And_Kill.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Die_And_Kill"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1080">
    <Window.Resources>
        <BitmapImage x:Key="Beach" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" 
                     UriSource="D:\repos/Die_And_KILL/Die_And_KILL/Resources/Beach.jpg"/>
    </Window.Resources>
    <Window.Background>
        <ImageBrush ImageSource="{DynamicResource Beach}" />
    </Window.Background>
    <Grid>
        
    </Grid>
</Window>
